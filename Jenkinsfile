pipeline {
    agent any
    stages {
        stage("clone") {
            steps {
                echo "this is a build stage"
            }
        }
    }
    stage("build") {
        steps {
            echo "this is a build stage"
        }
    }
    stage("deploy") {
        steps {
            echo "this is a deploy stage"
        }
    }
    }
}